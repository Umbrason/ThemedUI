            #include "Packages/com.unity.render-pipelines.core/ShaderLibrary/Color.hlsl"
            #include "Packages/com.unity.render-pipelines.universal/ShaderLibrary/Core.hlsl"
            #include "Packages/com.unity.render-pipelines.universal/ShaderLibrary/Lighting.hlsl"
            #include "Packages/com.unity.render-pipelines.universal/ShaderLibrary/ShaderGraphFunctions.hlsl"
            #include "Packages/com.unity.shadergraph/ShaderGraphLibrary/ShaderVariablesFunctions.hlsl"
        
            // --------------------------------------------------
            // Graph
        
            // Graph Properties
            CBUFFER_START(UnityPerMaterial)
            float _ColorCount;
            CBUFFER_END
            TEXTURE2D(_MainTex); SAMPLER(sampler_MainTex); float4 _MainTex_TexelSize;
            TEXTURE2D(_Palette); SAMPLER(sampler_Palette); float4 _Palette_TexelSize;
            SAMPLER(_SampleTexture2D_3C04D618_Sampler_3_Linear_Repeat);
            SAMPLER(_SampleTexture2D_ABD4E6CA_Sampler_3_Linear_Repeat);        
            
            // Graph Pixel
            struct SurfaceDescriptionInputs
            {
                float4 uv0;
                float4 VertexColor;
            };
            
            struct SurfaceDescription
            {
                float3 Color;
                float Alpha;
                float AlphaClipThreshold;
            };
            
            SurfaceDescription SurfaceDescriptionFunction(SurfaceDescriptionInputs IN)
            {
                SurfaceDescription surface = (SurfaceDescription)0;
                float2 paletteIndex = float2(((IN.VertexColor.r * 255 + .5f) / (float)_ColorCount),.5f);
                float4 Tint = SAMPLE_TEXTURE2D(_Palette, sampler_Palette, paletteIndex);
                float4 Color = SAMPLE_TEXTURE2D(_MainTex, sampler_MainTex, IN.uv0.xy);
                surface.Color = Tint.xyz;
                surface.Alpha = Color.w;
                surface.AlphaClipThreshold = 0;
                return surface;
            }
        
            // --------------------------------------------------
            // Structs and Packing
        
            // Generated Type: Attributes
            struct Attributes
